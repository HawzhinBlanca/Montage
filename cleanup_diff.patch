+                print(f"     â†’ {loc}")
+
+    # Write cleanup recommendations
+    with open("feature_flag_cleanup.txt", "w") as f:
+        f.write("# Feature Flag Cleanup Recommendations\n\n")
+
+        if dead_flags:
+            f.write("## Dead Feature Flags to Remove\n")
+            for flag in sorted(dead_flags):
+                f.write(f"- {flag}\n")
+            f.write("\n")
+
+        if legacy_patterns:
