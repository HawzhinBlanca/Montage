+def kill_oldest_ffmpeg() -> bool:
+    """Kill the oldest FFmpeg process to free memory"""
+    try:
+        # Find all FFmpeg processes
+        ffmpeg_procs = []
+        for proc in psutil.process_iter(['pid', 'name', 'create_time', 'cmdline']):
