+        for adr_file in adr_dir.glob("*.md"):
+            with open(adr_file) as f:
+                content = f.read()
+                # Look for file references
+                for py_file in Path("montage").rglob("*.py"):
+                    if str(py_file) in content or py_file.name in content:
+                        referenced_files.add(str(py_file))
+
