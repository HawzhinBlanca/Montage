+    # Legacy env vars
+    legacy_vars = ["USE_GPU", "MAX_WORKERS", "CACHE_TTL", "MAX_COST_USD"]
+    for var in legacy_vars:
+        if var in env_vars:
+            legacy_patterns.append({
+                "var": var,
+                "locations": env_vars[var],
+                "recommendation": "Migrate to structured settings"
