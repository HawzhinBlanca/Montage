+    RETURN NEW;
+END;
+$$ language 'plpgsql';
+
+-- Add updated_at columns and triggers where needed
+ALTER TABLE video_job ADD COLUMN IF NOT EXISTS updated_at TIMESTAMPTZ DEFAULT NOW();
